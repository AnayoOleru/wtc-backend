{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "jest": true
  },
  "extends": ["airbnb-base"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "no-console": 0,
    "function-paren-newline": 0,
    "arrow-parens": 0,
    "implicit-arrow-linebreak": 0,
    "global-require": 0,
    "operator-linebreak": 0,
    "arrow-body-style": 0,
    "object-curly-newline": 0,
    "no-param-reassign": 0,
    "camelcase": 0,
    "array-callback-return": 0
  }
}
